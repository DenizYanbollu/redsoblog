ate) return\n    if (state === \"SET\") return setEpisode(episode)\n    if (state === \"GET\") return currentEpisode\n  }\n\n  const getEpisode = () => {\n    if (!selectAnime(\"GET\")) return\n    if (!selectEpisode(\"GET\")) return\n    return selectAnime(\"GET\").episodes[selectEpisode(\"GET\")]\n  }\n\n  const getSourceList = () => {\n    if (getEpisode(\"GET\")) return getEpisode(\"GET\").sourceList\n  }\n\n  const getEpisodeList = () => {\n    if (selectAnime(\"GET\")) return selectAnime(\"GET\").episodes\n    else return []\n  }\n\n  const changePortal = (state) => {\n    if (state === \"login\") {setPortal(state); return}\n    if (portal === \"login\") {setPortal(false); return}\n    state ? setPortal(state) : setPortal(false)\n  }\n\n  const animeListed = (input) => {return <div onClick={() => selectAnime(\"SET\", input)} className=\"Searchbar-Anime\">{input}</div>}\n\n  const useTag = (state, tag = null) => {\n    if (state === \"SET\") {\n      axios.get(\"http://195.174.137.190:2005/search/category/\"+tag).then(({data}) => setListe(data.map(e => animeListed(e.title))))\n      return setQuery(tag)\n    }\n    if (state === \"GET\") {\n      return searchQuery\n    }\n  }\n\n  const isRoute = (route) => {\n    if (!route) return false\n    const routeExpresion = new RegExp('(.*): (.*)')\n    if (route.match(routeExpresion))  {\n      const [_, content, search] = route.match(routeExpresion)\n      return {state: true, type: content, search: search}\n    } else {\n      return false\n    }\n  }\n\n\n  const props = {\n    selectAnime,\n    selectEpisode,\n    getEpisode,\n    getSourceList,\n    getEpisodeList,\n    useTag,\n    setListe,\n    changePortal,\n    isRoute,\n    userInfo,\n    portal,\n    liste,\n  }\n\n  document.onkeydown = (e) => {\n    if (e.key === \"Escape\") {\n      console.log(e.key)\n    }\n  }\n\n  return (\n    <div className=\"App\">\n      <link rel=\"stylesheet\" href=\"https://pro.fontawesome.com/releases/v5.10.0/css/all.css\" 